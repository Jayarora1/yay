my_set = {1,2,2,3,3,4,4,5}
print(my_set)

my_set.add(6)
print(my_set)

set1 = my_set
set2 = {1,3,5}

print(set1)
print(set2)
print("difference of the two sets")
print(set1.difference(set2))
print("the symmetric difference of the two sets")
print(set1.symmetric_difference(set2))

setc1 = {"blue","green"}
setc2 = {"blue","yellow"}
sety = print(setc1.intersection(setc2))
setz = print(setc1.union(setc2))